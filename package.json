{
  "name": "bex",
  "version": "1.0.0",
  "description": "A simple Travel Route searcher",
  "main": "src/shared/application/http/implementations/express/app.ts",
  "scripts": {
    "build": "tsc",
    "ci": "npm run lint && npm run test && npm run build",
    "dev:console": "nodemon src/shared/infra/console/implementations/travel-route/app.ts",
    "dev:init": "ts-node src/shared/infra/console/implementations/init/app.ts",
    "dev:http": "nodemon src/shared/infra/http/implementations/express/app.ts",
    "start:console": "node dist/shared/infra/console/implementations/travel-route/app.js",
    "start:init": "node dist/shared/infra/console/implementations/init/app.js",
    "start:http": "node -r dotenv/config dist/shared/infra/http/implementations/express/app.js",
    "lint": "eslint src/** --ext .ts",
    "lint:fix": "eslint src/** --ext .ts --fix",
    "prebuild": "rimraf dist",
    "test": "npm run test:unit && npm run test:integration",
    "test:coverage": "nyc npm run test",
    "test:integration": "mocha --timeout 0 -r ts-node/register -r dotenv/config \"./**/*.integration.spec.ts\"",
    "test:unit": "mocha -t 10000 -p -r ts-node/register -r dotenv/config \"./**/*.unit.spec.ts\""
  },
  "keywords": [
    "test",
    "route",
    "travel"
  ],
  "author": "Roniel Ramos Gomes",
  "license": "ISC",
  "dependencies": {
    "celebrate": "^13.0.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/chai": "4.2.14",
    "@types/chai-as-promised": "7.1.3",
    "@types/cors": "2.8.8",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.9",
    "@types/helmet": "4.0.0",
    "@types/mocha": "8.0.4",
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "7.14.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "mocha": "8.2.1",
    "nodemon": "2.0.6",
    "nyc": "15.1.0",
    "source-map-support": "0.5.19",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  }
}
